					<div class="text-center tab-content">
						<div class="tab-pane fade show active" id="bikes">
							<div class="four-item single-products">

								<% topProducts.forEach((product)=>{ %>
								<div class="product-item-wrap">
									<div class="product-item">
										<div class="pro-img">
											<a href="/productDetails?id=<%= product._id %>"><img src="/admin/assets/images/product/sharpedproduct/<%= product.productImg[0] %>" width="241"
													height="195" alt="Product" /></a>
										</div>
										<div class="riding-title clearfix">
											<div class="product-title">
												<a href="/productDetails?id=<%= product._id %>">
													<h3 class="title"><%= product.productName %></h3>
												</a>
												<p>Price: <span> ₹  <%= product.productPrice %> </span></p>
											</div>
										</div>
									</div>
								</div>
								
								<% }) %>
							
							</div>
