		<!-- slider section end -->
		<!-- featured section start -->
		<section class="featured-area featured-one section-padding">
			<div class="container">
				<div class="row">
					<div class="col-md-8 col-lg-6 col-text-center">
						<div class="section-title text-center">
							<h2><span>Featured</span> products</h2>
							<div class="shape">
								<img src="img/icon/t-shape.png" width="16" height="16" alt="Title Shape" />
							</div>
							<p>It is a long established fact that a reader will be distracted by the readable content
								page when looking at its layout.</p>
						</div>
					</div>
				</div>
				<div class="row text-center">
					<div class="col-12">
						<div class="featured-slider single-products">

							<% featuredProduct.forEach((product)=>{ %>

							<div class="product-item-wrap">
								<div class="padding30">
									<div class="product-item">
										<div class="pro-img">
											<a href="/productDetails?id=<%= product._id %>"><img src="/admin/assets/images/product/sharpedproduct/<%= product.image[0] %>"
													width="318" height="194" alt="" /></a>
										</div>
										<div class="actions-btn">
											<ul class="clearfix">
												<li>
													<a href="cart.html"><i class="fa fa-shopping-cart"></i></a>
												</li>
												<li>
													<a href="#"><i class="fa fa-heart"></i></a>
												</li>
												<li>
													<a href="#" data-bs-toggle="modal" data-bs-target="#quick-view"><i
															class="fa fa-eye"></i></a>
												</li>
											</ul>
										</div>
										<div class="product-title">
											<a href="/productDetails?id=<%= product._id %>">
												<h3 class="title"><%= product.productName %></h3>
											</a>
											<p>Price <span>₹  <%= product.productPrice %></span></p>
										</div>
									</div>
								</div>
							</div>
							
							
							<% }) %>
							
						</div>
					</div>
				</div>
			</div>
		</section>
		<!-- featured section end -->
		
		<!-- riding section start -->
		<section class="riding-area riding-one section-padding">
			<div class="container">
				<div class="row">
					<div class="col-md-8 col-lg-6 col-text-center">
						<div class="section-title text-center">
							<h2><span>riding</span> accessories</h2>
							<div class="shape">
								<img src="img/icon/t-shape.png" width="16" height="16" alt="Title Shape" />
							</div>
							<p>It is a long established fact that a reader will be distracted by the readable content of
								a page when looking at its layout.</p>
						</div>
					</div>
				</div>
				<div class="row text-center">
					<div class="col-12">
						<div class="four-item riding-slider single-products">

							<% accessories.forEach((product)=>{ %>

							<div class="product-item-wrap">
								<div class="product-item">
									<div class="pro-img">
										<a href="/productDetails?id=<%= product._id %>"><img src="/admin/assets/images/product/sharpedproduct/<%= product.image[0] %>" width="263"
												height="282" alt="Product" /></a>
										
									</div>
									<div class="riding-title clearfix">
										<div class="product-title text-start floatleft">
											<a href="/productDetails?id=<%= product._id %>">
												<h3 class="title"><%= product.productName %></h3>
											</a>
											<p><span>₹  <%= product.productPrice %></span></p>
										</div>
									</div>
								</div>
							</div>
									
							<% }) %>
							
						</div>
					</div>
				</div>
			</div>
		</section>
		<!-- riding section end -->
	
		<!-- best-sell section start -->
		<section class="best-sell-area best-sell-one section-padding-top">
			<div class="container">
