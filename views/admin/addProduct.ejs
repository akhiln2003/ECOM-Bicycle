                                                            <option>Puma</option>
                                                            <option>Apple</option>
                                                            <option>Toyota</option>
                                                            <option>Toshiba</option>
                                                            <option>Artel</option>
                                                        </select>
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->
                                                
                                                <div class="row mb-4">
                                                    <label class="col-lg-3 col-form-label">Description*</label>
                                                    <div class="col-lg-9">
                                                        <textarea class="form-control" placeholder="Type here" rows="4"></textarea>
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->                                                                                            
                                                    <div class="row mb-4">
