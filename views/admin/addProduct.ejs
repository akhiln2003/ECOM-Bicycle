<%- include("../layout/admin/header.ejs") -%>  

        <div class="screen-overlay"></div>

        <aside class="navbar-aside" id="offcanvas_aside">
            <%- include("../layout/admin/sidbar.ejs") -%>
      </aside>
        <main class="main-wrap">
    
            <%- include("../layout/admin/navbar.ejs") -%>  
            <section class="content-main">
                <div class="row">
                    <div class="content-header">
                        <div>
                            <h2 class="content-title card-title">Add Product</h2>
                            <p>Add, edit or delete a Product</p>
                        </div>
                        <a href="/admin/products" class="btn btn-primary"><i class="material-icons md-arrow_back"></i>Go Back</a>
                    </div>
                    <div class="col-9" style="margin-left: 6rem; margin-top: 2rem;">
                        <div class="card">
                            <div class="card-body">
                                <div class="row gx-5">
                                   
                                    <div class="col-lg-9">
                                        <section class="content-body p-xl-4">
                                            <form>
                                                <div class="row mb-4">
                                                    <label class="col-lg-3 col-form-label">Product name*</label>
                                                    <div class="col-lg-9">
                                                        <input type="text" class="form-control" placeholder="Type here" />
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->
                                                <div class="row mb-4">
                                                    <label class="col-lg-3 col-form-label">Product Price*</label>
                                                    <div class="col-lg-9">
                                                        <input type="text" class="form-control" placeholder="Type here" />
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->
                                                <div class="row mb-4">
                                                    <label class="col-lg-3 col-form-label">Previous Price*</label>
                                                    <div class="col-lg-9">
                                                        <input type="text" class="form-control" placeholder="Type here" />
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->
                                                <div class="row mb-4">
                                                    <label class="col-lg-3 col-form-label">Quantity*</label>
                                                    <div class="col-lg-9">
                                                        <input type="text" class="form-control" placeholder="Type here" />
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->
                                                <div class="row mb-4">
                                                    <label class="col-lg-3 col-form-label" style="margin-top: 2rem;">Category & Brand *</label>
                                                    <div class="col-lg-4">
                                                        <small class="text-muted font-sm mb-10">Brand</small>
                                                        <select multiple size="4" class="form-control select-multiple">
                                                            <option>Adidas</option>
                                                            <option>Puma</option>
                                                            <option>Apple</option>
                                                            <option>Toyota</option>
                                                            <option>Toshiba</option>
                                                            <option>Artel</option>
                                                        </select>
                                                    </div>
                                                    <!-- col.// -->
                                                    <div class="col-lg-4">
                                                        <small class="text-muted font-sm mb-10">Category</small>
                                                        <select multiple size="4" class="form-control select-multiple">
                                                            <option>Adidas</option>
                                                            <option>Puma</option>
                                                            <option>Apple</option>
                                                            <option>Toyota</option>
                                                            <option>Toshiba</option>
                                                            <option>Artel</option>
                                                        </select>
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->
                                                
                                                <div class="row mb-4">
                                                    <label class="col-lg-3 col-form-label">Description*</label>
                                                    <div class="col-lg-9">
                                                        <textarea class="form-control" placeholder="Type here" rows="4"></textarea>
                                                    </div>
                                                    <!-- col.// -->
                                                </div>
                                                <!-- row.// -->                                                                                            
                                                    <div class="row mb-4">
                                                        <div class="col-md-6">
                                                            <div class="card">
                                                                <div class="card-header">
                                                                    <h4>Media 1</h4>
                                                                </div>
                                                                <div class="card-body">
                                                                    <div class="input-upload">
                                                                        <img src="assets/imgs/theme/upload.svg" alt="" />
                                                                        <input class="form-control" type="file" />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    
                                                        <div class="col-md-6">
                                                            <div class="card">
                                                                <div class="card-header">
                                                                    <h4>Media 2</h4>
                                                                </div>
                                                                <div class="card-body">
                                                                    <div class="input-upload">
                                                                        <img src="assets/imgs/theme/upload.svg" alt="" />
                                                                        <input class="form-control" type="file" />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    
                                                    <div class="row mb-4">
                                                        <div class="col-md-6">
                                                            <div class="card">
                                                                <div class="card-header">
                                                                    <h4>Media 3</h4>
                                                                </div>
                                                                <div class="card-body">
                                                                    <div class="input-upload">
                                                                        <img src="assets/imgs/theme/upload.svg" alt="" />
                                                                        <input class="form-control" type="file" />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    
                                                        <div class="col-md-6">
                                                            <div class="card">
                                                                <div class="card-header">
                                                                    <h4>Media 4</h4>
                                                                </div>
                                                                <div class="card-body">
                                                                    <div class="input-upload">
                                                                        <img src="assets/imgs/theme/upload.svg" alt="" />
                                                                        <input class="form-control" type="file" />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                <br />
                                                    <div>
                                                        <button class="btn btn-light rounded font-sm mr-5 text-body hover-up">Submit</button>
                                                        <button class="btn btn-light rounded font-sm mr-5 text-body hover-up ">Back</button>
                                                    </div>
                                                
                                            </form>
                                        </section>
                                        <!-- content-body .// -->
                                    </div>
                                    <!-- col.// -->
                                </div>
                                <!-- row.// -->
                            </div>
                            <!-- card body end// -->
                        </div>
                    </div>
                </div>
            </section>
            <!-- content-main end// -->
        </main>
        <%- include("../layout/admin/footer.ejs") -%>